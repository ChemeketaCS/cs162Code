#include <iostream>
#include <list>
#include <string>
using namespace std;

int main() {
  // Create a list of strings
  list<string> fruit = {"apple", "date", "banana", "cherry"};

  for (list<string>::iterator it = fruit.begin(); it != fruit.end(); ++it) {
    // Use *it to get the value the iterator points to
    string currentValue = *it;
    cout << currentValue << " ";
  }
  cout << endl;

  // Create an iterator "it" that points to the beginning of the list
  for (auto it = fruit.begin(); it != fruit.end(); ++it) {
    cout << *it << endl;
  }
  cout << endl;

  for (const auto& currentValue : fruit) {
    cout << currentValue << " ";
  }
  cout << endl;
}