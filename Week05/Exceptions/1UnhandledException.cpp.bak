// Division by zero.

#include <iostream>

using namespace std;

int main() {
  int dividend, divisor, quotient;

  cout << "Enter the dividend: ";
  cin >> dividend;
  cout << endl;

  cout << "Enter the divisor: ";
  cin >> divisor;
  cout << endl;

  quotient = dividend / divisor;
  cout << "Quotient = " << quotient << endl;

  return 0;
}
