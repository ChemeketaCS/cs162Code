#include <iostream>
using namespace std;

int binaryPatterns(int n, string patternSoFar = "") {
    if (n == 0) {
        cout << patternSoFar << endl;
        return 1; // Base case: one pattern of length 0 (the empty pattern)
    } else {
        int countWithZero = binaryPatterns(n - 1, patternSoFar + "0");
        int countWithOne = binaryPatterns(n - 1, patternSoFar + "1");
        return countWithZero + countWithOne;
    }
}

int main() {
    int count = binaryPatterns(3);
    cout << "Number of binary patterns of length 3: " << count << endl;
}
